{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["/node_modules","/build"],
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style":"off"
    }
}
